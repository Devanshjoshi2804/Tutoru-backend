{
  "version": 2,
  "builds": [
    {
      "src": "tutorU/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "tutorU/wsgi.py" }
  ]
}
{
  "rewrites": [
    {
      "source": "/admin/(.*)",
      "destination": "/api/admin/$1"
    },
    {
      "source": "/myapp/api/(.*)",
      "destination": "/api/myapp/api/$1"
    }
  ]
}
