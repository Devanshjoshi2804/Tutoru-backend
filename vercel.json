{
  "version": 2,
  "builds": [
    {
      "src": "tutorU/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/admin/(.*)", "dest": "/api/admin/$1" },
    { "src": "/myapp/api/(.*)", "dest": "/api/myapp/api/$1" },
    { "src": "/(.*)", "dest": "tutorU/wsgi.py" }
  ]
}
